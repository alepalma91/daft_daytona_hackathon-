# Daytona API Configuration
DAYTONA_API_KEY=your-api-key-here
DAYTONA_API_URL=https://api.daytona.io  # Optional: defaults to production URL
DAYTONA_TIMEOUT=300  # Optional: timeout in seconds

# Environment Configuration
DAYTONA_ENV=development  # development, staging, production
DEBUG=false

# Sandbox Configuration
SANDBOX_MAX_RUNTIME=600  # Maximum runtime in seconds
SANDBOX_MEMORY_LIMIT=512  # Memory limit in MB
SANDBOX_CPU_LIMIT=1  # CPU limit (number of cores)
